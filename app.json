						{
							"id": "0",
							"label": {
								"en": "Switch up = Off, Switch down = On.",
								"nl": "Schakelaar omhoog = Uit, Schakelaar omlaag = Aan."
							}
						},
						{
							"id": "1",
							"label": {
								"en": "Switch up = On, Switch down = Off.",
								"nl": "Schakelaar omhoog = Aan, Schakelaar omlaag = Uit."
							}
						}
					]
				},
				{
					"id": "poll_switch2",
					"type": "checkbox",
					"label": {
						"en": "Polling Right Switch",
						"nl": "Polling Rechter Schakelaar"
					},
					"hint": {
